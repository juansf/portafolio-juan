div(class="divider")
section.trabajos#trabajos.section
    .container.center-align
        h4.titulo-borde Trabajos
        h3.titulos-2 My Trabajos
        p.texto Conoce algunos de mis proyectos mas importantes
    .container-fluid
        .conten-menu-gallery.center-align.section
            .category-list
                a( class="categoria-item all active-gallery"  data-category="all") Todo 
                a( class="categoria-item corporativo" data-category="corporativo") Corporativo
                a( class="categoria-item diseno" data-category="diseno") Dise√±o
                a( class="categoria-item web" data-category="web") Web   
        
        .content-imagen
            -for(let i = 1; i<=4; i++)
                a(href=`imagenes/gallery/diseno-${i}.jpg` data-fancybox="gallery" data-caption="Caption #1" class="product-item z-depth-2 diseno" data-category="diseno")
                    img(src=`imagenes/gallery/diseno-${i}.jpg`, alt="diseno")
                a(href=`imagenes/gallery/corporativo-${i}.jpg` data-fancybox="gallery" data-caption="Caption #2" class="product-item z-depth-2 corporativo" data-category="corporativo")
                    img(src=`imagenes/gallery/corporativo-${i}.jpg`, alt="corporativo")    
                a(href=`imagenes/gallery/web-${i}.jpg` data-fancybox="gallery" data-caption="Caption #3" class="product-item z-depth-2 web" data-category="web")
                    img(src=`imagenes/gallery/web-${i}.jpg`, alt="web")